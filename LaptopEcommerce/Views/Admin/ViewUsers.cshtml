@{
    ViewData["Page"] = "Admin";
}

@model IEnumerable<LaptopEcommerce.Models.Customer>;

    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">Customer's id</th>
                    <th scope="col">Customer's name</th>
                    <th scope="col">loction</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.ID</th>
                        <td>@item.Name</td>
                        <td>@item.Location</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>